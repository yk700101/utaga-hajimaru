sopranoMusicI =
{
  % --------------------------------------------------
  % 1-1
  % --------------------------------------------------
  \relative c'
  {
    r2 f2 |
    f4 g4 a2 |
    c2 bes2 |
    a2 r4
  }

  % --------------------------------------------------
  % 1-2
  % --------------------------------------------------
  \relative c''
  {
    a4 |
    c4. bes8 g2 |
    a2 f2 |
    e2 r4
  }

  % --------------------------------------------------
  % 1-3
  % --------------------------------------------------
  \relative c'
  {
    f4 |
    g4 f4 e4 f4 |
    g2 r4
  }

  % --------------------------------------------------
  % 1-4
  % --------------------------------------------------
  \relative c''
  {
    g4 |
    f4 g4 a2 |
    c2 bes2 |
    a2 r4
  }

  % --------------------------------------------------
  % 1-5
  % --------------------------------------------------
  \relative c''
  {
    a4 |
    c4. bes8 g2 |
    a2 f2 |
    e2 r4
  }

  % --------------------------------------------------
  % 1-6
  % --------------------------------------------------
  \relative c'
  {
    e4 |
    f4 g4 a2 |
    bes4. a8 g8 [ f8 ] g4~ |
    g8 [ f8 ] f2 e4 |
    f2 r4
  }

  % --------------------------------------------------
  % 1-7
  % --------------------------------------------------
  \relative c'
  {
    e4 |
    f4 g4 a2 |
    bes4. a8 g8 [ f8 ] g4~ |
    g8 [ f8 ] f2 e4 |
    f1*2/2 |
  }
}

sopranoMusic =
{
  \time 2/2

  \sopranoMusicI
}

