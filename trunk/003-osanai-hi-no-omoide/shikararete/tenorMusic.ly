tenorMusic =
{
  \set Staff.instrumentName = "Tenor"

  \clef "treble_8" 

  \key c \major

  % --------------------------------------------------
  % Initial Setting
  % --------------------------------------------------
%  \set Staff.midiInstrument = "acoustic grand"
  \set Staff.midiInstrument = "violin"

  \override Staff.TimeSignature #'style = #'()

  \time 4/4

  % --------------------------------------------------
  % 1-1
  % --------------------------------------------------
  \relative c'
  {
    R1 | R1 | R1 | R1 | R1 |
  }

  % --------------------------------------------------
  % 1-2
  % --------------------------------------------------
  \relative c'
  {
    r8. [ c16~ ] c2 r4 | r8. [ a16~ ] a2 r4 | r8. [ c16~ ] c2 r4 | r8 [ c~ ] c4 d4 r8 [ e ] |
  }

  % --------------------------------------------------
  % 1-3
  % --------------------------------------------------
  \relative c''
  {
    g2 e4 d4 | c8 [ c ] d8 [ c ] a4 g4 | e8 [ e ] g8 [ a ] c4 g4 | a2. r4 |
  }

  % --------------------------------------------------
  % 1-4
  % --------------------------------------------------
  \relative c'
  {
    r4 c16 [ r8. ] r4 c16 [ r8. ] | r4 a16 [ r8. ] r4 b16 [ r8. ] | r4 c16 [ r8. ] r2 | \times2/3{ r8 a c~ } c2~ c4 |
  }

  % --------------------------------------------------
  % 1-5
  % --------------------------------------------------
  \relative c'
  {
    r4 e2.~ | e2. r4 | r8 [ a, ] c8 [ d ] e4~ e8 [ c ] | d2.~ d8 [ e ] | d2. r8 [ e, ] |
  }

  % --------------------------------------------------
  % 2-1
  % --------------------------------------------------
  \relative c'
  {
    g2 a4 a4 | g2. r4 | R1 | r2. r8 [ e' ] |
  }

  % --------------------------------------------------
  % 2-2
  % --------------------------------------------------
  \relative c'
  {
    e2 e4 d4 | c4~ c8 [ c ] a4 a4 | g4~ g8 [ g ] c4 d8 [ e ] | d2. r4 |
  }

  % --------------------------------------------------
  % 2-3
  % --------------------------------------------------
  \relative c'''
  {
    R1 | R1 | R1 | R1 |
  }

  % --------------------------------------------------
  % 2-4
  % --------------------------------------------------
  \relative c'
  {
    R1 | r2 r4 d4 | e2 e4 c4 | b2. r4 |
  }

  % --------------------------------------------------
  % 2-5
  % --------------------------------------------------
  \relative c'
  {
    e2 f4~ f8 [ e ] | g4 e4 e4~ e8. [ r16 ] | c1~ | c2 b4 a4~ | a2. r4 |
  }

  \bar "|."
}

